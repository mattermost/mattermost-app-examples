{
	"app_id": "hello-lifecycle",
	"display_name": "Hello, Lifecycle!",
	"homepage_url": "https://github.com/mattermost/mattermost-app-examples/golang/lifecycle",
	"requested_permissions": [
		"act_as_bot"
	],
	"on_install": {
		"path": "/install",
		"expand": {
			"acting_user": "id"
		}
	},
	"on_version_changed": {
		"path": "/version_changed",
		"expand": {
			"acting_user": "id"
		}
	},
	"on_uninstall": {
		"path": "/uninstall",
		"expand": {
			"acting_user": "id"
		}
	},
	"on_enable": {
		"path": "/enable",
		"expand": {
			"acting_user": "id"
		}
	},
	"on_disable": {
		"path": "/disable",
		"expand": {
			"acting_user": "id"
		}
	}
}
